{
  "productName": "Theme-o-rama",
  "version": "0.1.0",
  "identifier": "net.kackman.theme-o-rama",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1425",
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Theme-o-rama",
        "width": 1150,
        "height": 750,
        "resizable": true,
        "maximized": false,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "window-state": null
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "NQJQRYZZG3"
    }
  },
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}